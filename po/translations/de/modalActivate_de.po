msgstr "Geplantes Startdatum: {{Mitglied.MitgliedschaftAktivierungAn}}"

#: app/views/modalActivate.html:109
msgid "Select date"
msgstr "Datum wählen"

#: app/views/modalActivate.html:101
msgid "Set activation date"
msgstr "Startdatum festlegen"

#: app/views/modalActivate.html:94
msgid "Submit new payment data"
msgstr "Teile uns die neuen Zahldaten mit."

#: app/views/modalActivate.html:127
msgid "Thank you!"
msgstr "Danke!"

